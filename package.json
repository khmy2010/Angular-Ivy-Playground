{
  "name": "angular-ivy",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --aot=true --buildOptimizer=true --progress=true",
    "lint": "ng lint",
    "e2e": "concurrently \"npm start\" \"npx cypress open\"",
    "c": "npx git-cz",
    "p": "git push -u origin master",
    "gc": "ng g c shared/loading-splash --skipImport=true --selector=app-loading-splash --skipTests=true -c=OnPush",
    "cypress:open": "cypress open",
    "ci:start-server": "angular-http-server --path ./dist/angular-ivy -p 4200",
    "ci:cy": "cypress run",
    "ci:verify-cy": "cypress verify",
    "ci:test": "start-server-and-test ci:start-server http://localhost:4200 ci:cy"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.2",
    "@angular/common": "~8.1.2",
    "@angular/compiler": "~8.1.2",
    "@angular/core": "~8.1.2",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.1.2",
    "@angular/platform-browser": "~8.1.2",
    "@angular/platform-browser-dynamic": "~8.1.2",
    "@angular/router": "~8.1.2",
    "bulma": "^0.7.5",
    "firebase": "^6.3.1",
    "rxjs": "~6.5.2",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.2",
    "@angular/cli": "~8.1.2",
    "@angular/compiler-cli": "~8.1.2",
    "@angular/language-service": "~8.1.2",
    "@types/jasmine": "~3.3.14",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.6.8",
    "angular-http-server": "^1.9.0",
    "codelyzer": "^5.0.0",
    "concurrently": "^4.1.1",
    "cypress": "^3.4.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.2.0",
    "karma-chrome-launcher": "~3.0.0",
    "karma-coverage-istanbul-reporter": "~2.0.6",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.0",
    "start-server-and-test": "^1.9.1",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~3.5.3"
  }
}
